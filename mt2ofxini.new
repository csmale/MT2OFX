Structured86=0
SkipEmptyMemoFields=0
ScriptFile=RaiffeisenbankZurich.vbs

[COOPCHBB]
BankName=Coop Bank Switzerland
Structured86=1
SkipEmptyMemoFields=0
ScriptFile=CoopCH.vbs

[RAIFCH22]
BankName=Raiffeisenbank (CH)
Structured86=0
SkipEmptyMemoFields=0
ScriptFile=RaiffeisenbankZurich.vbs

[CRESCH22]
BankName=Credit Suisse
Structured86=0
SkipEmptyMemoFields=0
ScriptFile=Credit Suisse.vbs

[CRESCHZZ]
BankName=Credit Suisse
Structured86=0
SkipEmptyMemoFields=0
ScriptFile=Credit Suisse.vbs

[RBABCH22]
